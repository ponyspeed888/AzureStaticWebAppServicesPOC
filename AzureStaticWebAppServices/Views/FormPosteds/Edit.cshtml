@model AzureStaticWebAppServices.Models.FormPosted

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>FormPosted</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="FormPostedId" />
            <div class="form-group">
                <label asp-for="ClientId" class="control-label"></label>
                <select asp-for="ClientId" class="form-control" asp-items="ViewBag.ClientId"></select>
                <span asp-validation-for="ClientId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FormData" class="control-label"></label>
                <input asp-for="FormData" class="form-control" />
                <span asp-validation-for="FormData" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PostedTime" class="control-label"></label>
                <input asp-for="PostedTime" class="form-control" />
                <span asp-validation-for="PostedTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

