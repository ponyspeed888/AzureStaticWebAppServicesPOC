@model IEnumerable<AzureStaticWebAppServices.Models.FormPosted>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FormData)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PostedTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Client)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FormData)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PostedTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Client.ClientUrl)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FormPostedId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FormPostedId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FormPostedId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
